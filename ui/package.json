{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "country-flag-icons": "^1.5.5",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.1.4",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-tabs": "^6.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm install --production && npm run build",
    "start:dev": "npm install &&  npm run build:dev",
    "build": "env-cmd -f .env.prod react-scripts build && env-cmd -f .env.prod  react-scripts start",
    "build:dev": "env-cmd -f .env.local react-scripts build && env-cmd -f .env.local react-scripts start",
    "install:deps:ui": "npm install --production",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
